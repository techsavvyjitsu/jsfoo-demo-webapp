{
  "name": "jsfoo18-demo-webapp",
  "version": "1.0.0",
  "description": "This application is built to demo GraphQL client for webapps using React, Apollo-Client",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development node --inspect ./scripts/start.js",
    "start:prod": "serve -s build",
    "build": "node scripts/build.js",
    "build:ci": "npm run lint && npm run test && npm run build",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint --fix ./src",
    "postinstall": "npm audit fix && npm run build",
    "precommit": "lint-staged"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=9.3.0",
    "npm": ">=6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techsavvyjitsu/jsfoo18-demo-webapp.git"
  },
  "keywords": [
    "graphql",
    "jsfoo-2018",
    "apollo",
    "client"
  ],
  "author": "Ashwin Hegde <ashwin.hegde3@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/techsavvyjitsu/jsfoo18-demo-webapp/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.20",
    "apollo-boost": "^0.1.7",
    "bulma": "^0.7.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.0",
    "resolve": "1.6.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "^0.28.11",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "^1.13.5",
    "style-loader": "^0.19.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write \"{*config*.js,package.json,src,__{tests,mocks}__}/**/*.js\"",
      "git add"
    ],
    "*.json": [
      "prettier --write \"{*config*.js,package.json,src,__{tests,mocks}__}/**/*.js\"",
      "git add"
    ]
  }
}
